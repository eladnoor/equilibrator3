@article{wehrens_size_2018,
	title = {Size {Laws} and {Division} {Ring} {Dynamics} in {Filamentous} {Escherichia} coli cells},
	volume = {28},
	issn = {0960-9822},
	url = {https://www.cell.com/current-biology/abstract/S0960-9822(18)30160-X},
	doi = {10.1016/j.cub.2018.02.006},
	abstract = {{\textless}h2{\textgreater}Summary{\textless}/h2{\textgreater}{\textless}p{\textgreater}Our understanding of bacterial cell size control is based mainly on stress-free growth conditions in the laboratory [1–10]. In the real world, however, bacteria are routinely faced with stresses that produce long filamentous cell morphologies [11–28]. \textit{Escherichia coli} is observed to filament in response to DNA damage [22–25], antibiotic treatment [11–14, 28], host immune systems [15, 16], temperature [17], starvation [20], and more [18, 19, 21], conditions which are relevant to clinical settings and food preservation [26]. This shape plasticity is considered a survival strategy [27]. Size control in this regime remains largely unexplored. Here we report that \textit{E. coli} cells use a dynamic size ruler to determine division locations combined with an adder-like mechanism to trigger divisions. As filamentous cells increase in size due to growth, or decrease in size due to divisions, its multiple Fts division rings abruptly reorganize to remain one characteristic cell length away from the cell pole and two such length units away from each other. These rules can be explained by spatiotemporal oscillations of Min proteins. Upon removal of filamentation stress, the cells undergo a sequence of division events, randomly at one of the possible division sites, on average after the time required to grow one characteristic cell size. These results indicate that \textit{E. coli} cells continuously keep track of absolute length to control size, suggest a wider relevance for the adder principle beyond the control of normally sized cells, and provide a new perspective on the function of the Fts and Min systems.{\textless}/p{\textgreater}},
	language = {English},
	number = {6},
	urldate = {2018-08-02},
	journal = {Current Biology},
	author = {Wehrens, Martijn and Ershov, Dmitry and Rozendaal, Rutger and Walker, Noreen and Schultz, Daniel and Kishony, Roy and Levin, Petra Anne and Tans, Sander J.},
	month = mar,
	year = {2018},
	pmid = {29502951, 29502951},
	pages = {972--979.e5}
}

@article{tanner_four_2018,
	title = {Four {Key} {Steps} {Control} {Glycolytic} {Flux} in {Mammalian} {Cells}},
	volume = {7},
	issn = {2405-4712},
	url = {http://www.sciencedirect.com/science/article/pii/S2405471218302436},
	doi = {10.1016/j.cels.2018.06.003},
	abstract = {Summary
Altered glycolysis is a hallmark of diseases including diabetes and cancer. Despite intensive study of the contributions of individual glycolytic enzymes, systems-level analyses of flux control through glycolysis remain limited. Here, we overexpress in two mammalian cell lines the individual enzymes catalyzing each of the 12 steps linking extracellular glucose to excreted lactate, and find substantial flux control at four steps: glucose import, hexokinase, phosphofructokinase, and lactate export (and not at any steps of lower glycolysis). The four flux-controlling steps are specifically upregulated by the Ras oncogene: optogenetic Ras activation rapidly induces the transcription of isozymes catalyzing these four steps and enhances glycolysis. At least one isozyme catalyzing each of these four steps is consistently elevated in human tumors. Thus, in the studied contexts, flux control in glycolysis is concentrated in four key enzymatic steps. Upregulation of these steps in tumors likely underlies the Warburg effect.},
	number = {1},
	urldate = {2018-08-02},
	journal = {Cell Systems},
	author = {Tanner, Lukas Bahati and Goglia, Alexander G. and Wei, Monica H. and Sehgal, Talen and Parsons, Lance R. and Park, Junyoung O. and White, Eileen and Toettcher, Jared E. and Rabinowitz, Joshua D.},
	month = jul,
	year = {2018},
	keywords = {GLUT, MCT1, PFK, PFKFB, flux control, glycolysis, metabolic control analysis, metabolism, metabolomics, optogenetics},
	pages = {49--62.e8}
}

@article{peres_thermodynamic_2018,
