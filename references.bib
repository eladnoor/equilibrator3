	number = {5},
	urldate = {2019-10-24},
	journal = {Cell Systems},
	author = {Christodoulou, Dimitris and Link, Hannes and Fuhrer, Tobias and Kochanowski, Karl and Gerosa, Luca and Sauer, Uwe},
	month = may,
	year = {2018},
	keywords = {allosteric regulation, central metabolism, kinetic modeling, metabolomics, oxidative stress},
	pages = {569--578.e7}
}

