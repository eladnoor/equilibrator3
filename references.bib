@article{stolzenberger_characterization_2013,
	title = {Characterization of fructose 1,6-bisphosphatase and sedoheptulose 1,7-bisphosphatase from the facultative ribulose monophosphate cycle methylotroph {Bacillus} methanolicus},
	issn = {0021-9193, 1098-5530},
	url = {http://jb.asm.org/content/early/2013/09/03/JB.00672-13},
	doi = {10.1128/JB.00672-13},
	abstract = {The genome of the facultative ribulose monophosphate cycle methylotroph Bacillus methanolicus encodes two bisphosphatases, one on the chromosome (GlpXC) and one (GlpXP) one on plasmid pBM19 which is required for methylotrophy. Both enzymes were purified from recombinant E. coli and shown to be active as fructose 1,6-bisphosphatases (FBPases). The FBPase-negative Corynebacterium glutamicum mutant Δfbp could be phenotypically complemented with glpXC and glpXP from B. methanolicus. GlpXP and GlpXC share similar functional properties, as they were here found to be active as homotetramers in vitro, activated by Mn2+ ions and inhibited by Li+, but differed in terms of the kinetic parameters. GlpXC showed much higher catalytic efficiency and a lower KM for fructose 1,6-bisphosphate (86.3 s—1 mM—1 and 14±0.5 μM) than GlpXP (8.8 s—1 mM—1 and 440±7.6 μM), indicating that GlpXC is the major FBPase of B. methanolicus. Both enzymes were tested for activity as sedoheptulose 1,7-bisphosphatase (SBPase) since a SBPase variant of the ribulose monophosphate cycle has been proposed for B. methanolicus. The substrate for SBPase reaction, sedoheptulose 1,7-bisphosphate, could be synthesized in vitro using both fructose 1,6-bisphosphate aldolase proteins from B. methanolicus. Evidence for activity as SBPase could be obtained for GlpXP, but not for GlpXC. Based on these in vitro data GlpXP is a promiscuous SBPase/FBPase and might function in the RuMP cycle of B. methanolicus.},
	language = {en},
	urldate = {2018-07-31},
	journal = {Journal of Bacteriology},
	author = {Stolzenberger, Jessica and Lindner, Steffen N. and Persicke, Marcus and Brautaset, Trygve and Wendisch, Volker F.},
	month = sep,
	year = {2013},
	pmid = {24013630},
	pages = {JB.00672--13}
}

@article{mulleder_functional_2016,
	title = {Functional {Metabolomics} {Describes} the {Yeast} {Biosynthetic} {Regulome}},
