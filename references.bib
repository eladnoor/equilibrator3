@article{schuster_minimization_1991,
	title = {Minimization of intermediate concentrations as a suggested optimality principle for biochemical networks},
	volume = {29},
	issn = {1432-1416},
	url = {https://doi.org/10.1007/BF00160470},
	doi = {10.1007/BF00160470},
	abstract = {The criterion of minimum intermediate concentrations in steady states is suggested to be of essential relevance in the evolution of biochemical reaction networks. This extremum principle is phrased in two different ways, firstly in terms of total osmolarity of intermediates and, secondly, as a multiple criterion problem. The relationships between the two problems are elucidated and a solving method for the latter is then given. It turns out that in each optimal state, the network can be subdivided into a slow and a fast subsystem. The notion of convex conservation relations is introduced and the implications of such relations for the optimization problem are investigated.},
