	keywords = {Cancer, Cell Nucleus, Cell Proliferation, Colon, Colonic Neoplasms, Computer architecture, Convolutional neural network, Feature extraction, Histocytochemistry, Humans, Image Interpretation, Computer-Assisted, Machine Learning, Machine learning, Microprocessors, NEP, Neural Networks (Computer), SC-CNN, Shape, Tumors, biological organs, biomedical optical imaging, cancer, cancerous tissue, cell nuclei classification, cell nuclei detection, cellular biophysics, cellular heterogeneity, colorectal adenocarcinoma images, dataset, deep learning, eosin stain, high-probability values, highest average F1 score, histology image analysis, histopathology images, image classification, joint detection, learning (artificial intelligence), locality sensitive deep learning, medical image processing, neighboring ensemble predictor, nucleus detection, probability, quantitative analysis, routine colon cancer histology images, spatially constrained convolutional neural network, standard hematoxylin, tissue constituents, tumours, whole-slide images},
	pages = {1196--1206}
}

@article{diether_towards_2017,
	series = {Antimicrobials * {Bacterial} {Systems} {Biology}},
	title = {Towards detecting regulatory protein–metabolite interactions},
	volume = {39},
	issn = {1369-5274},
	url = {http://www.sciencedirect.com/science/article/pii/S1369527417300164},
