@article{jansen_d1-d2_1999,
	title = {D1-{D}2 protein degradation in the chloroplast},
	volume = {260},
	url = {http://dx.doi.org/10.1046/j.1432-1327.1999.00196.x},
	doi = {10.1046/j.1432-1327.1999.00196.x},
	abstract = {The D1 and D2 proteins of the photosystem II (PSII) reaction center are stable in the dark, while rapid degradation occurs in the light. Thus far, a quantitative correlation between degradation and photon fluences has not been determined. In Spirodela oligorrhiza, D1-D2 degradation increases with photon flux. We find that kinetics for D2 degradation mirror those for D1, except that the actual half-life times of the D2 protein are about three times larger than those of the D1. The degradation ratio, D2/D1, is fluence independent, supporting the proposal [Jansen, M.A.K., Greenberg, B.M., Edelman, M., Mattoo, A.K. \& Gaba, V. (1996), Photochem. Photobiol.63, 8142013817] that degradation of the two proteins is coupled. It is commonly conceived that D1 degradation is predominantly associated with photon fluences that are supersaturating for photosynthesis. We now show that a fluence as low as 5 µmol·m22122·s22121 elicited a reaction constituting \&gt; 25\% of the total degradation response, while \&gt; 90\% of the degradation potential was attained at intensities below saturation for photosynthesis (2248750 µmol·m22122·s22121). Thus, in intact plants, D1 degradation is overwhelmingly associated with fluences limiting for photosynthesis. D1 degradation increases with photon flux in a complex, multiphasic manner. Four phases were uncovered over the fluence range from 020131600 µmol·m22122·s22121. The multiphasic saturation kinetics underscore that the D1 and D2 degradation response is complex, and emanates from more than one parameter. The physiological processes associated with each phase remain to be determined.},
	number = {2},
	urldate = {2010-07-06},
	journal = {European Journal of Biochemistry},
	author = {Jansen, Marcel A. K. and Mattoo, Autar K. and Edelman, Marvin},
	year = {1999},
	pages = {527--532}
}

