	keywords = {Acyl Coenzyme A, Amino Acid Sequence, Base Sequence, Butyryl-CoA Dehydrogenase, Chemistry, Physical, Cloning, Molecular, DNA, Bacterial, Escherichia coli, Gene Expression, Genes, Bacterial, Kinetics, Molecular Sequence Data, Molecular Weight, Oxidation-Reduction, Physicochemical Phenomena, Protein Structure, Quaternary, Pseudomonas putida, Recombinant Proteins, Sequence Homology, Amino Acid, Spectrophotometry, Substrate Specificity},
