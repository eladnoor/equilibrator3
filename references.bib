}

@article{muller_resource_2015,
	title = {Resource allocation in metabolic networks: kinetic optimization and approximations by {FBA}},
	volume = {43},
	copyright = {© 2015 Authors; published by Portland Press Limited},
	issn = {0300-5127, 1470-8752},
	shorttitle = {Resource allocation in metabolic networks},
	url = {http://www.biochemsoctrans.org/content/43/6/1195},
	doi = {10.1042/BST20150156},
	abstract = {Based on recent theoretical results on optimal flux distributions in kinetic metabolic networks, we explore the congruences and differences between solutions of kinetic optimization problems and results obtained by constraint-based methods. We demonstrate that, for a certain resource allocation problem, kinetic optimization and standard flux balance analysis (FBA) give rise to qualitatively different results. Furthermore, we introduce a variant of FBA, called satFBA, whose predictions are in qualitative agreement with kinetic optimization.},
	language = {en},
	number = {6},
	urldate = {2018-08-20},
	journal = {Biochemical Society Transactions},
	author = {Müller, Stefan and Regensburger, Georg and Steuer, Ralf},
	month = dec,
