@article{levac_application_2008,
	title = {Application of carborundum abrasion for investigating the leaf epidermis: molecular cloning of {Catharanthus} roseus 16-hydroxytabersonine-16-{O}-methyltransferase},
	volume = {53},
	issn = {0960-7412},
	shorttitle = {Application of carborundum abrasion for investigating the leaf epidermis},
