}

@article{gillon_naturally_2000,
	title = {Naturally low carbonic anhydrase activity in {C}$_{\textrm{4}}$ and {C}$_{\textrm{3}}$ plants limits discrimination against {C}$^{\textrm{18}}${OO} during photosynthesis},
	volume = {23},
	url = {http://dx.doi.org/10.1046/j.1365-3040.2000.00597.x},
	doi = {10.1046/j.1365-3040.2000.00597.x},
	abstract = {The 18O content of CO2 is a powerful tracer of photosynthetic activity at the ecosystem and global scale. Due to oxygen exchange between CO2 and 18O-enriched leaf water and retrodiffusion of most of this CO2 back to the atmosphere, leaves effectively discriminate against 18O during photosynthesis. Discrimination against 18O (039418O) is expected to be lower in C4 plants because of low ci and hence low retrodiffusing CO2 flux. C4 plants also generally show lower levels of carbonic anhydrase (CA) activities than C3 plants. Low CA may limit the extent of 18O exchange and further reduce 039418O. We investigated CO22013H2O isotopic equilibrium in plants with naturally low CA activity, including two C4 (Zea mays, Sorghum bicolor) and one C3 (Phragmites australis) species. The results confirmed experimentally the occurrence of low 039418O in C4, as well as in some C3, plants. Variations in CA activity and in the extent of CO22013H2O isotopic equilibrium (03B8eq) estimated from on-line measurements of 039418O showed large range of 02013100\% isotopic equilibrium (03B8eq= 020131). This was consistent with direct estimates based on assays of CA activity and measurements of CO2 concentrations and residence times in the leaves. The results demonstrate the potential usefulness of 039418O as indicator of CA activity in vivo. Sensitivity tests indicated also that the impact of 03B8eq\&lt; 1 (incomplete isotopic equilibrium) on 18O of atmospheric CO2 can be similar for C3 and C4 plants and in both cases it increases with natural enrichment of 18O in leaf water.},
	number = {9},
	urldate = {2010-06-24},
	journal = {Plant, Cell \& Environment},
	author = {Gillon, J. S. and Yakir, D.},
	year = {2000},
	pages = {903--915}
}

