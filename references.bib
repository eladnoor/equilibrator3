	journal = {Science Advances},
	author = {Hiramatsu, Kotaro and Ideguchi, Takuro and Yonamine, Yusuke and Lee, SangWook and Luo, Yizhi and Hashimoto, Kazuki and Ito, Takuro and Hase, Misa and Park, Jee-Woong and Kasai, Yusuke and Sakuma, Shinya and Hayakawa, Takeshi and Arai, Fumihito and Hoshino, Yu and Goda, Keisuke},
